        {
            "_id": "LBWQXoQFG0Rg2Q9Y",
            "name": "Faerie Curse",
            "type": "feature",
            "img": "icons/svg/item-bag.svg",
            "data": {
                "ability": "",
                "actionOptions": [
                    "savingThrow"
                ],
                "activation": {
                    "cost": 1,
                    "type": "action",
                    "reactionTrigger": ""
                },
                "attack": {
                    "bonus": "",
